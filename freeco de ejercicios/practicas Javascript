function bucleWhile(num){
    let i = 10;
while ( i< num ){
  
  console.log(i)
  i++
}
}
 bucleWhile(11)
/////////////////do while se ejecuta asi la condicion no se cumpla, al menos una vez
function doWhile(num1){
let i =11;
  do{
    console.log(i)
    i++
    }while(i<num1)

}
doWhile(11)

//for (inicializacion, condicion y actualizaciÃ³n)
function forBucle(num2){
  for(let i=0; i < num2 ; i++){
  console.log(i);
}  
}
forBucle(5)